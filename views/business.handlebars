
<article class="container">
    <div class="row bootstrap snippets bootdeys"> 
        <div class="col-md-9 col-sm-7"> 
            <h2>Businesses</h2> 
        </div> 
        
    </div>
<ol>
    {{#each Businesses}}
    <li>
        
    <div class="member-entry"> 
        <a href="#" class="member-img"> 
            <img src="{{businesses.logo_url}}" class="img-rounded"> 
            <i class="fa fa-forward"></i> 
        </a> 
        <div class="member-details"> 
            <h4>{{businesses.b_name}}</h4> 
            <div class="row info-list"> 
                <div class="col-sm-4"> 
                    <i class="fa fa-briefcase"></i>
                    {{businesses.b_description}}
                </div> 
                
                <div class="col-sm-4"> 
                    <i class="fa fa-envelope"></i> 
                    <a href="#">{{businesses.b_email}}</a> 
                </div> 
                <div class="col-sm-4"> 
                    <p>{{businesses.b_email}}</p> 
                </div>
            </div> 
        </div> 
    </div>
</article>
    </li>
    {{/each}}
</ol>
