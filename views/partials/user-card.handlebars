<li>
        
    <div class="member-entry"> 
        
        <div class="member-details"> 
            <div class="row info-list"> 
                <div class="col-sm-4"> 
                    <h4>{{first_name}} &nbsp {{last_name}}</h4>
                    <i class="fa fa-briefcase"></i>
                    {{description}}
                </div> 
                
                <div class="col-sm-4"> 
                    <h4>Email</h4>
                    <i class="fa fa-envelope"></i> 
                    <a href="#">{{email}}</a> 
                </div> 
                <div class="col-sm-4"> 
                    <h4>Impairment</h4>
                    <p>{{impairs.[0].impairment}}{{user_impairment}}</p>

                </div>
                <div class="col-sm-12"> 
                    <h4>suggestions</h4>
                    <p>{{suggestions.[0].suggestion_text}}</p>

                </div>
            </div> 
        </div> 
    </div>
    </li>